@model dynamic
@{
    var commandResponse = ViewData["_CommandResponse"] as RiotHangfireDemo.Domain.CommandResponse;
    var messages = commandResponse?.Messages?.Count > 0
        ? string.Join("<br />", commandResponse.Messages)
        : null;
}
@if (commandResponse?.IsSuccess == true && messages != null)
{
    <pre class="alert alert-success message">@Html.Raw(messages)</pre>
}
else if (commandResponse?.IsSuccess == false && messages != null)
{
    <pre class="alert alert-danger message">@Html.Raw(messages)</pre>
}
